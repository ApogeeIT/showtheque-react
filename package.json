    "ts-loader": "^2.0.0",
    "tslint": "^4.0.1",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.8.4",
